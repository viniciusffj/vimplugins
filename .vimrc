syntax enable

set number
set hlsearch
set incsearch
set autoindent
set background=dark
set ignorecase

set cursorline
hi CursorLine ctermbg=236 cterm=none

noremap  <Up> ""
noremap! <Up> <Esc>
noremap  <Down> ""
noremap! <Down> <Esc>
noremap  <Left> ""
noremap! <Left> <Esc>
noremap  <Right> ""
noremap! <Right> <Esc>
